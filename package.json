  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jquery": "^3.5.29",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-diff": "^2.0.3",
    "eslint-plugin-prettier": "^5.1.3",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  }
